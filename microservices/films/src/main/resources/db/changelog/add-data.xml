<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="1_data_suprun" author="suprun" runOnChange="true">
        <loadUpdateData tableName="positions" primaryKey="id" file="db/data/positions.csv"/>
        <loadUpdateData tableName="stars" primaryKey="id" file="db/data/stars.csv"/>
        <loadUpdateData tableName="films" primaryKey="id" file="db/data/films.csv"/>
        <loadUpdateData tableName="film_staff" primaryKey="film_id,star_id,position_id" file="db/data/film_staff.csv"/>
    </changeSet>
</databaseChangeLog>